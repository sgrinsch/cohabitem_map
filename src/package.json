{
  "name": "geosearch",
  "version": "0.0.0",
  "description": "testing browserify npm et al",
  "main": "static/js/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "beefy src/js/app.js:../assets/static/js/bundle.js --live",
    "bundle": "browserify js/app1.js -o ../assets/static/js/bundle1.js  &&  browserify js/app2.js -o ../assets/static/js/bundle2.js",
    "bundle3": "browserify js/app3.js -o ../assets/static/js/bundle3.js",
    "add": "browserify src/js/app.js -o ../assets/static/js/bundle.js && git add .",
    "build-css": "node-sass --includepath scss sass/main.sass ../assets/static/css/styles.min.css --output-style compressed"
  },
  "author": "sgrinschpun",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:sgrinschpun/cohabitem_map.git"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-table": "^1.11.2",
    "drmonty-leaflet-awesome-markers": "^2.0.2",
    "esri-leaflet": "^2.0.7",
    "esri-leaflet-geocoder": "^2.2.3",
    "hover.css": "^2.2.0",
    "jquery": "^3.1.1",
    "leaflet": "^1.0.3",
    "leaflet-draw": "^0.4.9",
    "leaflet-easybutton": "^2.2.0",
    "leaflet.locatecontrol": "^0.60.0",
    "node-sass": "^4.5.1"
  },
  "devDependencies": {
    "beefy": "^2.1.8",
    "browserify": "^14.1.0"
  }
}
